{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter",
      "type": "dart",
      "request": "launch",
      "program": "example/lib/main.dart"
    },
    {
      "name": "(lldb) Launch",
      "type": "lldb",
      // "type": "cppdbg",
      "request": "launch",
      "sourceFileMap": {
        "../..": "${workspaceFolder}"
      },
      "osx": {
        "MIMode": "lldb",
        "program": "${workspaceFolder}/example/build/macos/Build/Products/Debug/system_tray_example.app",
      },
      "linux": {
        "MIMode": "gdb",
        "program": "${workspaceFolder}/example/build/linux/x64/debug/bundle/system_tray_example",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
      },
      "args": [],
      "stopAtEntry": true,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
    },
    {
      "name": "(Windows) Launch",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/example/build/windows/runner/Debug/system_tray_example.exe",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${fileDirname}",
      "environment": []
    }
  ]
}